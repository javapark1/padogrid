<?xml version="1.0"?>

<cache-config
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.oracle.com/coherence/coherence-cache-config"
	xsi:schemaLocation="http://xmlns.oracle.com/coherence/coherence-cache-config
   coherence-cache-config.xsd">

	<defaults>
		<serializer>pof</serializer>
	</defaults>

	<caching-scheme-mapping>

		<!-- Maps for PBM payload and transaction tests -->
		<cache-mapping>
			<cache-name>contacts</cache-name>
			<scheme-name>distributed</scheme-name>
			<key-type>com.tangosol.examples.pof.ContactId</key-type>
			<value-type>com.tangosol.examples.pof.Contact</value-type>
		</cache-mapping>
		<cache-mapping>
			<cache-name>eligibility</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>profile</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>summary</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>

		<!-- nw maps -->
		<cache-mapping>
			<cache-name>/nw/categories</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>/nw/customers</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>/nw/employees</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>/nw/employee_territories</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>/nw/orders</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>/nw/order_details</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>/nw/products</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>/nw/regions</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>/nw/shippers</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>/nw/suppliers</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>/nw/territories</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>

		<!-- maps for group tests -->
		<cache-mapping>
			<cache-name>map1</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>map2</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>map3</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>map4</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>map5</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>map6</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>map7</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>map8</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>map9</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>map10</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>map11</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>map12</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>map13</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>map14</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>map15</cache-name>
